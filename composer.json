{
    "name": "repository/project",
    "description": "Your PHP project",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "twbs/bootstrap": "^5.3",
        "twbs/bootstrap-icons": "^1.11"
    },
    "require-dev": {
        "npm-asset/apexcharts": "^3.45"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "oomphinc/composer-installers-extender": true,
            "fxp/asset-plugin": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "minimum-stability": "stable"
}
